  ProductReview_1.Rating,
  Products_0.Name,
  Sum(Products_0.Price) AS TotalPrice
FROM (localized_com_logali_Products AS Products_0 LEFT JOIN com_logali_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
